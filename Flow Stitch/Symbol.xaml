<Window x:Class="Flow_Stitch.Symbol"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Flow_Stitch"
        mc:Ignorable="d"
        Title="Pattern Symbols" Height="594" Width="874.4">
    <Grid Margin="0,0,-2,1" Background="#FFD8FAFF">
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="852*"/>
            <ColumnDefinition Width="9*"/>
            <ColumnDefinition Width="4*"/>
        </Grid.ColumnDefinitions>
        <Image x:Name="image" HorizontalAlignment="Left" Height="490" Margin="10,10,0,0" VerticalAlignment="Top" Width="817" Stretch="Uniform" RenderOptions.BitmapScalingMode="NearestNeighbor"/>
        <Button x:Name="Savebutton" Content="Save" Margin="740,521,0,0" Background="#FFEFD3FF" Click="Savebutton_Click" Height="26" VerticalAlignment="Top" FontSize="14" HorizontalAlignment="Left" Width="99"/>

    </Grid>
</Window>
